        <select class="form-control" id="school_year" name="school_year">
            <% (Time.now + 1.year).year.downto(2014).each do |yr| %>
             <option <%= 'selected' if params[:school_year] == (yr-1).to_s + "-" + yr.to_s %>><%= (yr-1).to_s + "-" + yr.to_s %></option>
