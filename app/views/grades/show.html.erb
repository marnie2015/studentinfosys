<script>
    $(document).ready(function() {
        //click save
        $(".save").click(function() {
            var nme = $(this).data("name");
            var school_year = $("#school_year option:selected").val();

            $(".edit" + nme).removeClass("hidden");
            $(".save" + nme).addClass("hidden");
            $(".cancel" + nme).addClass("hidden");

            $.get("/save-grade?" + nme + "=" + $("#" + nme).val() + "&student_id=<%= params[:id] %>" + "&school_year=" + school_year, function(res) {
                alert(res);
                $("#" + nme).attr("disabled", true);
            });
        });
    })
</script>